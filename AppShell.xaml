    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProiectFinal_Mobile"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
            Title="Welcome"
            ContentTemplate="{DataTemplate local:MainPage}"
        />
        <!--<ShellContent
            Title="Contact"
            ContentTemplate="{DataTemplate local:AboutPage}"
        />-->
        <ShellContent
            Title="My Animals"
            ContentTemplate="{DataTemplate local:ListEntryPage}"
        />
        <ShellContent
            Title="Rooms"
            ContentTemplate="{DataTemplate local:RoomListEntryPage}"
        />
    </TabBar>
</Shell>
